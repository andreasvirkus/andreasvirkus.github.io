<nav id="menu" class="menu" role="navigation">
    <button class="menu__handle" title="Open menu"><span>Menu</span></button>
    <div class="menu__inner">
        <ul class="menu__list">
            {{#each collections.pages}}
            {{#isnot this.path 'cv' }}
            {{#isnot this.path '404' }}
            <li class="menu__item" {{#is this.path @root.path }}aria-current="page"{{/is}}>
                {{#if this.path }}
                <a href="/{{ this.path }}/" class="menu__link">{{ this.title }}</a>
                {{ else }}
                <a href="/" class="menu__link">{{ this.title }}</a>
                {{/if}}
            </li>
            {{/isnot}}
            {{/isnot}}
            {{/each}}
        </ul>
    </div>
    <div class="morph-shape" data-morph-open="M300-10c0,0,295,164,295,410c0,232-295,410-295,410" data-morph-close="M300-10C300-10,5,154,5,400c0,232,295,410,295,410">
        <svg width="100%" height="100%" viewBox="0 0 600 800" preserveAspectRatio="none">
            <path fill="none" d="M300-10c0,0,0,164,0,410c0,232,0,410,0,410"/>
        </svg>
    </div>
</nav>
